{
  "name": "hlgilesphotography",
  "private": true,
  "scripts": {
    "dev": "concurrently -n server,client -c blue,green \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "npm --prefix client run dev",
    "dev:server": "docker compose -f server/docker-compose.yml -f server/docker-compose.override.yml up -d",
    "stop": "docker compose -f server/docker-compose.yml -f server/docker-compose.override.yml down",
    "build:client": "npm --prefix client run build",
    "install:client": "npm --prefix client install"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
